package BL;

public class p1 {
	private String name;
	private int empid;
	private static int id;
	protected double salary;
	
	static
	{
		id=10;
	}
	
	
	private double netsal()
	{
		return salary;
	}

	public p1() {
		super();
		this.name="Akshay";
		this.salary=10000;
		empid=id++;
	}

	public p1(String name, double salary) {
		super();
		this.name = name;
		this.salary = salary;
		empid=id++;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getSalary() {
		return salary;
	}

	public void setSalary(double salary) {
		this.salary = salary;
	}

	public int getEmpid() {
		return empid;
	}

	@Override
	public String toString() {
		return "p1 [name=" + name + ", empid=" + empid + ", salary=" + salary + "]";
	}
	
	
	
	
	
}
